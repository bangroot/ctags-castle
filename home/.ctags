--langdef=groovy
--langmap=groovy:.groovy
--regex-groovy=/^\s*[(private|public|protected|final|synchronized|abstract)[ \t]*]*(def|[A-Za-z0-9_.]+)[ \t]+([A-Za-z0-9_"]+)[ \t]*\(/\2/m,method/
--regex-groovy=/^[ \t]*private[ \t]+(final)?[ \t]*(def|[a-zA-Z0-9_.]+)[ \t]+([A-Za-z0-9_]+)($|[ \t]+=)/\3/f,field/
--regex-groovy=/^[ \t]*(public)?(final)?[ \t]*(def|[a-zA-Z0-9_.]+)[ \t]+([A-Za-z0-9_]+)($|[ \t]+=)/\4/p,property/
--regex-groovy=/^\s*[(private|public|abstract|static)[ \t]*]*class[ \t]+([A-Za-z0-9_]+)\s*/\1/c,class/
--regex-groovy=/^\s*[(private|public|abstract|static)[ \t]*]*enum[ \t]+([A-Za-z0-9_]+)\s*/\1/e,enum/
--regex-groovy=/^\s*[(private|public|abstract|static)[ \t]*]*trait[ \t]+([A-Za-z0-9_]+)\s*/\1/t,trait/
--regex-groovy=/^\s*[(private|public|abstract|static)[ \t]*]*interface[ \t]+([A-Za-z0-9_]+)\s*/\1/i,interface/

--langdef=gradle
--langmap=gradle:.gradle
--regex-gradle=/^\s*task\s+([a-zA-Z0-9]+)/\1/t,task/
